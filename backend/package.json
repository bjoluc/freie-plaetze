{
  "name": "backend",
  "private": true,
  "scripts": {
    "dev": "npm run start-dependencies && keystone dev",
    "seed": "npm run dev -- --seed",
    "e2e": "npm run start-dependencies && keystone build && npm run migrate:reset && keystone start",
    "start": "npm run migrate:deploy && keystone start",
    "build": "keystone build",
    "keystone-postinstall": "keystone postinstall",
    "migrate:deploy": "keystone prisma migrate deploy",
    "migrate:draft": "keystone prisma migrate dev --create-only",
    "migrate:reset": "keystone prisma migrate reset --force",
    "start-dependencies": "docker compose up -d db db-e2e imgproxy"
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@keystone-6/auth": "6.0.0",
    "@keystone-6/core": "4.0.1",
    "@keystone-6/fields-document": "6.0.0",
    "@next/env": "^13.1.1",
    "@turf/helpers": "^6.5.0",
    "axios": "^1.2.2",
    "email-templates": "^10.0.1",
    "graphql-upload": "^15.0.2",
    "imgproxy": "^1.0.0",
    "lodash": "^4.17.21",
    "nodemailer": "^6.8.0",
    "pug": "^3.0.2",
    "slugify": "^1.6.5",
    "undici": "^5.14.0"
  },
  "devDependencies": {
    "@types/email-templates": "^10.0.1",
    "@types/nodemailer": "^6.4.7",
    "type-fest": "^3.5.1",
    "typescript": "^4.9.4"
  }
}
